#include "p2x8c4m64p.common.spin2h"

CON
    { --- clock settings --- }
    _xtlfreq    = 20_000_000
    _clkfreq_def= 180_000_000

    { --- pin definitions --- }
    { 8x blue LEDs }
    LED1        = 56
    LED2        = 57
    LED3        = 58
    LED4        = 59
    LED5        = 60
    LED6        = 61
    LED7        = 62
    LED8        = 63

    { microSD socket - SPI }
    SD_MISO     = 58
    SD_MOSI     = 59
    SD_CS       = 60
    SD_CLK      = 61

    { SPI flash }
    FLASH_DO    = 58
    FLASH_DI    = 59
    FLASH_CLK   = 60
    FLASH_CS    = 61

DAT
